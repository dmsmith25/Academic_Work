	public static long QueueList(String[] data) {
		QueueLi test = new QueueLi();
		for(var i = 0; i < data.length; i++) {
			if(data[i].charAt(0) == 'i') {
				test.enqueue(data[i].charAt(1));
			}
		}
		
		 
			
	}
	

